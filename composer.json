{
    "name": "i-lateral/ss4-recipe",
    "type": "silverstripe-recipe",
    "description": "SilverStripe recipe for fully featured page and asset content editing",
    "homepage": "http://silverstripe.org",
    "license": "BSD-3-Clause",
    "require": {
        "silverstripe/recipe-plugin": "^1",
        "silverstripe/assets": "1.0.*@stable",
        "silverstripe/config": "1.0.*@stable",
        "silverstripe/framework": "4.0.*@stable",
        "silverstripe/asset-admin": "1.0.1@stable",
        "silverstripe/cms": "4.0.1@stable",
        "silverstripe/errorpage": "1.0.1@stable",
        "silverstripe/blog": "master",
        "silverstripe/userforms": "5.*",
        "silverstripe/widgets": "master",
        "ilateral/silverstripe-bootstrap-4": "dev-master",
        "axllent/silverstripe-scaled-uploads": "^2.1",
        "i-lateral/silverstripe-childhubpage": "^2.0",
        "jonom/focuspoint": "ss4-upgrade-dev"
    },
    "require-dev": {
        "phpunit/PHPUnit": "^5.7"
    },
    "extra": {
        "project-files": [
            ".htaccess",
            "app/*",
            "themes/custom/*",
            "index.php",
            "install.php",
            "install-frameworkmissing.html"
        ]
    },
    "config": {
        "process-timeout": 600
    },
    "prefer-stable": true,
    "minimum-stability": "dev"
}
