{
    "name": "i-lateral/ss4-recipe",
    "type": "silverstripe-recipe",
    "description": "SilverStripe recipe for fully featured page and asset content editing",
    "homepage": "http://silverstripe.org",
    "license": "BSD-3-Clause",
    "repositories": [
        {
            "type": "vcs",
            "url": "git@github.com:Makreig/Silverstripe-SEO.git"
        }
    ],
    "require": {
        "silverstripe/recipe-plugin": "^1",
        "silverstripe/recipe-cms": "4.4@stable",
        "silverstripe/admin": "1.1.2@stable",
        "silverstripe/asset-admin": "1.1.2@stable",
        "silverstripe/campaign-admin": "1.1.2@stable",
        "silverstripe/cms": "4.1.2@stable",
        "silverstripe/errorpage": "1.1.2@stable",
        "silverstripe/graphql": "1.1.2@stable",
        "silverstripe/reports": "4.1.2@stable",
        "silverstripe/siteconfig": "4.1.2@stable",
        "silverstripe/versioned": "1.1.2@stable",
        "silverstripe/blog": "^3.0",
        "silverstripe/userforms": "5.*",
        "silverstripe/widgets": "^2.0",
        "ilateral/silverstripe-bootstrap-4": "^1.0",
        "axllent/silverstripe-scaled-uploads": "^2.1",
        "i-lateral/silverstripe-childhubpage": "^2.0",
        "jonom/focuspoint": "^3.0",
        "heyday/silverstripe-gridfieldversionedorderablerows": "ss4-upgrade-dev",
        "symbiote/silverstripe-gridfieldextensions": "^3.1",
        "i-lateral/silverstripe-custommenus": "^2.0",
        "fractas/cookiepolicy": "^2.0",
        "i-lateral/silverstripe-socialnav": "^2.0",
        "i-lateral/silverstripe-deferedimages": "^1.0",
        "undefinedoffset/silverstripe-nocaptcha": "^2.0",
        "cyber-duck/silverstripe-seo": "4.3.x-dev"
    },
    "require-dev": {
        "phpunit/PHPUnit": "^5.7"
    },
    "extra": {
        "project-files": [
            ".htaccess",
            "app/*",
            "themes/custom/*",
            "index.php",
            "web.config"
        ]
    },
    "config": {
        "process-timeout": 600
    },
    "prefer-stable": true,
    "minimum-stability": "dev"
}
